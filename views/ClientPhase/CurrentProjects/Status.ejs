<!DOCTYPE html>
<html lang="en">

<head>

<% include ../Partials/ClientStyles %>

</head>

<body>

    <div id="wrapper">

       <% include ../Partials/ClientNavBars %>

        <div id="page-wrapper" style="height:700px !important;">

            <div class="container-fluid">

                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">Current Projects 
                            <small>Status</small>
                        </h1>
                       
                    </div>
                </div>
                <!-- /.row --><br/><br/><br/><br/>
				<div class="row">
                    <div class="col-lg-3">
                        <label>Progress</label>
                       
					</div>
					 <div class="col-lg-6">
                       	<div class="progress">
						<div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: <%=percent%>%;">
									<%=percent%>%
						</div>
					</div>
                       
					</div>
					
                </div>
				<div class="row">
                    <div class="col-lg-3">
                        <label>Deadline</label>
                       
					</div>
					 <div class="col-lg-6">
                       	<label><%=dead%></label>
					</div>
                       
					</div>
					
                </div>
				<div class="row" style="margin-top: 10%;">
                    <div class="col-lg-6">
                        <button type="button" class="btn btn-primary" id="pendingDeviceBtn" style="float:right;">Pending Device</button>
                       
					</div>
					 <div class="col-lg-6">
                       <button type="button" class="btn btn-primary" id="completedDeviceBtn">Completed Device</button>
					</div>
                       
					</div>
					
                </div>
				
				
			</div>
            <!-- /.container-fluid -->

        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->
     
	 	<!-- Pending Devices PopUp -->
	<div id="pendingDeviceModal" class="modal fade pendingDeviceModal" role="dialog" >
		<div class="modal-dialog">

    <!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 class="modal-title">Pending Devices</h4>
				</div>
			<div class="modal-body">
                <div style="width:100%;height:220px;overflow-y:auto;">
					<div style="float:left;width:50%;">
						<div class="col-sm-12">
                        <ul class="list-group">

                        <%var i=0;pen_devices.forEach(function(device){%>  
                        		<%i++;if(i==1){%>
                            <li class="list-group-item active"><%=device%></li>
                            <%}else{%>
                            <li class="list-group-item"><%=device%></li>
                            <%}%>
                         	<%});%>
                        </ul>
						</div>
					</div>
					<%if(pen_device_dt){%>
					<div style="float:left;width:50%;">
						<div class="col-sm-6"><label>Name:</label></div>
						<div class="col-sm-6"><label><%=pen_device_dt.dev_name%></label></div>
						<div class="col-sm-6"><label>OS:</label></div>
						<div class="col-sm-6"><label><%=pen_device_dt.dev_os%></label></div>
						<div class="col-sm-6"><label>Versions:</label></div>
						<div class="col-sm-6"><label><%=pen_device_dt.dev_os_ver%></label></div>
						<div class="col-sm-6"><label>PROCER SPP:</label></div>
						<div class="col-sm-6"><label><%=pen_device_dt.dev_proc_spd_gz%></label></div>
						<div class="col-sm-6"><label>CON Type:</label></div>
						<div class="col-sm-6"><label><%=pen_device_dt.dev_con%></label></div>
						<div class="col-sm-6"><label>RAM:</label></div>
						<div class="col-sm-6"><label><%=pen_device_dt.dev_ram_gb%></label></div>
						<div class="col-sm-6"><label>Battery:</label></div>
						<div class="col-sm-6"><label><%=pen_device_dt.dev_bat_mAh%></label></div>
						<div class="col-sm-6"><label>Status:</label></div>
						<div class="col-sm-6"><label>Pending</label></div>

					</div>
					<%}%>
				</div>
			</div>
	
    </div>

  </div>
</div>
	<!-- /Pending Devices PopUp -->
    <!-- Completed Devices PopUp -->
	<div id="completedDeviceModal" class="modal fade completedDeviceModal" role="dialog" >
		<div class="modal-dialog">

    <!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 class="modal-title">Completed Devices</h4>
				</div>
			<div class="modal-body">
                 <div style="width:100%;height:220px;overflow-y:auto;">
					<div style="float:left;width:50%;">
						<div class="col-sm-12">
                        <ul class="list-group">
                        	<%var i=0;com_devices.forEach(function(device){%>  
                        		<%i++;if(i==1){%>
                            <li class="list-group-item active"><%=device%></li>
                            <%}else{%>
                            <li class="list-group-item"><%=device%></li>
                            <%}%>
                         	<%});%>
                        </ul>
						</div>
					</div>
					<%if(com_device_dt){%>
					<div style="float:left;width:50%;">
						<div class="col-sm-6"><label>Name:</label></div>
						<div class="col-sm-6"><label><%=com_device_dt.dev_name%></label></div>
						<div class="col-sm-6"><label>OS:</label></div>
						<div class="col-sm-6"><label><%=com_device_dt.dev_os%></label></div>
						<div class="col-sm-6"><label>Versions:</label></div>
						<div class="col-sm-6"><label><%=com_device_dt.dev_os_ver%></label></div>
						<div class="col-sm-6"><label>PROCER SPP:</label></div>
						<div class="col-sm-6"><label><%=com_device_dt.dev_proc_spd_gz%></label></div>
						<div class="col-sm-6"><label>CON Type:</label></div>
						<div class="col-sm-6"><label><%=com_device_dt.dev_con%></label></div>
						<div class="col-sm-6"><label>RAM:</label></div>
						<div class="col-sm-6"><label><%=com_device_dt.dev_ram_gb%></label></div>
						<div class="col-sm-6"><label>Battery:</label></div>
						<div class="col-sm-6"><label><%=com_device_dt.dev_bat_mAh%></label></div>
						<div class="col-sm-6"><label>Status:</label></div>
						<div class="col-sm-6"><label>Completed</label></div>

					</div>
					<%}%>
				</div>
			</div>
	
    </div>

  </div>
</div>
	<!-- /Pending Devices PopUp -->
	
    <!-- jQuery -->
    <script src="js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

    <!-- Morris Charts JavaScript -->
    <script src="js/plugins/morris/raphael.min.js"></script>
    <script src="js/plugins/morris/morris.min.js"></script>
    <script src="js/plugins/morris/morris-data.js"></script>
	<script src="js/jquery-1.9.1.min.js"></script>
	<script src="js/jquery-1.11.3.min.js"></script>
	<script src="js/bootstrap-3.3.5.min.js"></script>
	<script>
	$(document).ready(function(){
		$("#pendingDeviceBtn").click(function(){
			
			$("#pendingDeviceModal").modal();
		});
		$("#completedDeviceBtn").click(function(){
			
			$("#completedDeviceModal").modal();
		});
	});
	</script>

</body>

</html>
