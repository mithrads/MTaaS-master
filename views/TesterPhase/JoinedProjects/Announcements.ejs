	<!-- Reply PopUp -->
	<div id="replyModal" class="modal fade replyModal" role="dialog" >
		<div class="modal-dialog">

    <!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header replyModalHeader">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 class="modal-title">Annoncement Reply</h4>
				</div>
			<div class="modal-body">
                <form role="form">
					<div class="form-group">
                         <textarea class="form-control" rows="3"></textarea>
                    </div>
				</form>
			</div>
		<div class="modal-footer">
		    <button type="button" class="btn btn-primary" id="replySubmitBtn">Reply</button>
			<button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
		</div>
    </div>

  </div>
</div>
	<!-- /Reply PopUp -->
	
	
    <!-- jQuery -->
    <script src="/js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="/js/bootstrap.min.js"></script>

