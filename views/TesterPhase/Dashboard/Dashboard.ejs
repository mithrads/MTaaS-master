<!DOCTYPE html>
<html lang="en">

<head>

<% include ../Partials/TesterStyles %>

</head>

<body>

    <div id="wrapper">

       <% include ../Partials/TesterNavBars %>

        <div id="page-wrapper" style="height:700px !important;">

            <div class="container-fluid">

                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">Dashboard 
                           
                        </h1>
                       
                    </div>
                </div>
                <!-- /.row -->
				
				<div class="row">
				<div class="col-lg-12">
				<div class="table-responsive" style="margin-top: 2%;">
                            <table class="table table-bordered table-hover" style="width: 75%;margin-left: 5%;">
                                <thead>
                                    <tr>
                                        <th>Project Name</th>
                                        <th>Deadline</th>
										<th>Details</th>
										<th>Join</th>
                                        
						             </tr>
                                </thead>
                                <tbody>
                                <%projs.forEach(function(proj){ var i=0%>
                                <%if(proj.approved)  {%>
                                    <%jprojs.forEach(function(jproj){%> 
                                   <%if(proj.proj_id==jproj) i=1;%>
                                     <%});%>
                                    <%if(i==0){%>
                                    <tr>
                                        <td><%=proj.proj_name%></td>
										<td><%=proj.deadline%></td>                                       
                                        <td style="width:12%">
										<button type="button" class="btn btn-primary" id="<%=proj.proj_id%>">Details</button>
										</td>
										<td style="width:12%">
                                        <form method="" action="">
                                        <input type="hidden" name="sourcepg" value="joinpr">
                                        <input type="hidden" name="proj_id" value="<%=proj.proj_id%>">
                                        <input type="submit" value="Join" class="btn btn-primary"></input>
										</form>
										</td>
                                    </tr>
                                    <%}%>
                                    <%}%>
                                <%});%>
                                </tbody>
                            </table>
				</div>
                </div>
                </div>
            <!-- /.container-fluid -->
            </div>
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->
<!-- Project Details PopUp -->
	<div id="projectDetailsModal" class="modal fade projectDetailsModal" role="dialog" >
		<div class="modal-dialog">

    <!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header projectDetailsModalHeader">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 class="modal-title">Project Detail</h4>
				</div>
			<div class="modal-body" style="overflow-y:auto;">
                	<div class="col-lg-6">

                        <form role="form">
			
                            <div class="form-group">
							    <label class="">Project Name:</label>
                            </div>
							 <div class="form-group">
                                <label class="">Deadline:</label>
                             </div>
							 <div class="form-group">
                                <label class="">URL:</label>
                             </div>
							 <div class="form-group">
                                <label class="">Award/Bug:</label>
                             </div>
							<div class="form-group">
                                <label class="">Description:</label>
                             </div>
							
							 
						</form>
					</div>
					<div class="col-lg-6">

                        <form role="form">
			
                             <div class="form-group">
							    <label class="">Project 1</label>
                            </div>
							 <div class="form-group">
                                <label class="">26-11-2015</label>
                             </div>
							 <div class="form-group">
                                <label class="">http://localhost:1926</label>
                             </div>
							 <div class="form-group">
                                <label class="">10</label>
                             </div>
							<div class="form-group">
                                <label class="">Test Mania</label>
                            </div>
							 
						</form>
				    </div>
			
			 </div>
		
            </div>

        </div>
    </div>
	<!-- /Device Details PopUp -->
    <!-- jQuery -->
    <script src="/js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="/js/bootstrap.min.js"></script>

    <!-- Morris Charts JavaScript -->
    <script src="/js/plugins/morris/raphael.min.js"></script>
    <script src="/js/plugins/morris/morris.min.js"></script>
    <script src="/js/plugins/morris/morris-data.js"></script>
	<script src="/js/jquery-1.9.1.min.js"></script>
	<script src="/js/jquery-1.11.3.min.js"></script>
	<script src="/js/bootstrap-3.3.5.min.js"></script>
	<script>
	$(document).ready(function(){
		$("#projectDetails1Btn").click(function(){
			
			$("#projectDetailsModal").modal();
		});
		
	});
	</script>

</body>

</html>
